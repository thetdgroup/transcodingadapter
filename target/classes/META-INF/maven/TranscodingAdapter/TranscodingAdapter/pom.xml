<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>TranscodingAdapter</groupId>
  <artifactId>TranscodingAdapter</artifactId>
  <version>0.0.1-SNAPSHOT</version>
  
  <properties>
   <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
  </properties>
    
  <build>
   <sourceDirectory>src</sourceDirectory>
 
  <plugins>
    <plugin>
      <artifactId>maven-compiler-plugin</artifactId>
      <version>3.0</version>
      <configuration>
        <source>1.6</source>
        <target>1.6</target>
      </configuration>
    </plugin>
   </plugins>
  </build>
  
  <dependencies>
   <dependency>
    <groupId>BaseEMailAdapter</groupId>
    <artifactId>BaseEMailAdapter</artifactId>
    <version>0.0.1-SNAPSHOT</version>
   </dependency>
   
   <dependency>
    <groupId>BaseTDGAdapter</groupId>
    <artifactId>BaseTDGAdapter</artifactId>
    <version>0.0.1-SNAPSHOT</version>
   </dependency>
   <dependency>
    <groupId>FuzeInCommunication</groupId>
    <artifactId>FuzeInCommunication</artifactId>
    <version>0.0.1-SNAPSHOT</version>
   </dependency>
   <dependency>
    <groupId>FuzeInConstants</groupId>
    <artifactId>FuzeInConstants</artifactId>
    <version>0.0.1-SNAPSHOT</version>
   </dependency>
   <dependency>
    <groupId>ServiceLogger</groupId>
    <artifactId>ServiceLogger</artifactId>
    <version>0.0.1-SNAPSHOT</version>
   </dependency>
   <dependency>
    <groupId>ServiceLogParser</groupId>
    <artifactId>ServiceLogParser</artifactId>
    <version>0.0.1-SNAPSHOT</version>
   </dependency>
   <dependency>
    <groupId>org.jdom</groupId>
    <artifactId>jdom</artifactId>
    <version>1.1.3</version>
   </dependency>
   <dependency>
    <groupId>jaxen</groupId>
    <artifactId>jaxen</artifactId>
    <version>full</version>
   </dependency>
   <dependency>
    <groupId>org.json</groupId>
    <artifactId>json</artifactId>
    <version>20090211</version>
   </dependency>
   
   <dependency>
    <groupId>org.apache.commons</groupId>
    <artifactId>commons-email</artifactId>
    <version>1.3</version>
   </dependency>
 
  </dependencies>
</project>